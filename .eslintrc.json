{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["simple-import-sort", "prettier"],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "multiline": "last",
        "reservedFirst": true
      }
    ],
    "prettier/prettier": "error",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}
